{
  "name": "note-students",
  "version": "1.0.0",
  "description": "Веб-приложение, позволяющее отмечать студентов на занятиях",
  "main": "src/server/app.js",
  "scripts": {
    "start": "npm-run-all start:*",
    "build": "npm-run-all build:*",
    "start:client": "node ./scripts/start-client.js",
    "build:client": "node ./scripts/build-client.js",
    "start:server": "nodemon --exec babel-node ./src/server/app.ts --config-file ./babel-node.config.js --extensions .ts --debug",
    "build:server": "babel ./src/server --out-dir ./build/server --config-file ./babel-node.config.js --extensions .ts",
    "lint:js": "eslint ./src/ --ext .js,.jsx,.ts,.tsx",
    "lint:css": "stylelint ./src/**/*.{css,pcss}",
    "lint": "npm run lint:js && npm run lint:css",
    "type-check": "tsc",
    "type-check:watch": "npm run type-check --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run type-check"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "src/client/**/*.{css,pcss}": [
      "prettier --write",
      "stylelint --fix --config stylelint.format.config.js",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Intellcet/Note-Students.git"
  },
  "author": "Intellcet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Intellcet/Note-Students/issues"
  },
  "homepage": "https://github.com/Intellcet/Note-Students#readme",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.5.5",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/qrcode.react": "^0.8.2",
    "axios": "^0.19.0",
    "clsx": "^1.0.4",
    "core-js": "^3.2.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http": "0.0.0",
    "md5": "^2.2.1",
    "qrcode.react": "^0.9.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.11.1",
    "react-router-dom": "^5.0.1",
    "regenerator-runtime": "^0.13.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.3.0",
    "sqlite3": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
